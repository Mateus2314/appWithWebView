<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".financiamentocadastrar">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="30dp">

        <ProgressBar
            android:id="@+id/myProgressBardad"
            android:layout_weight="0.1"
            style="@style/Base.Widget.AppCompat.ProgressBar.Horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <ImageView
            android:id="@+id/myImageViewDd"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.9"
            android:contentDescription="@string/webviewparacadastrodefinanciamento"
            app:srcCompat="@mipmap/ic_launcher" />


    </LinearLayout>

    <WebView
        android:id="@+id/wbmudarfinanciamento"
        android:layout_width="match_parent"
        android:layout_height="425dp">

    </WebView>


    <Button
        android:id="@+id/btvoltarfinanciamentocadastro"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline24" />





</LinearLayout>